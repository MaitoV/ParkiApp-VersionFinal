@model MVCBasic.Models.Cochera

@{
    ViewData["Title"] = "Create";
}

<h1>Create</h1>

<h4>Cochera</h4>
<hr />
<div class="row">
    <div class="col-md-4">
        <form asp-action="Create">
            <div asp-validation-summary="ModelOnly" class="text-danger"></div>
            <div class="form-group">
                <label asp-for="NumeroCochera" class="control-label"></label>
                <input asp-for="NumeroCochera" class="form-control" />
                <span asp-validation-for="NumeroCochera" class="text-danger"></span>
            </div>
            <div class="form-group">
                <label asp-for="Piso" class="control-label"></label>
                <!--<select asp-for="Piso" class="form-control"></select>-->
                <input asp-for="Piso" class="form-control" />
                <span asp-validation-for="Piso" class="text-danger"></span>
            </div>
            <div class="form-group">
                <label asp-for="FechaIngreso" class="control-label"></label>
                <input asp-for="FechaIngreso" class="form-control" />
                <span asp-validation-for="FechaIngreso" class="text-danger"></span>
            </div>
            <div class="form-group">
                <label asp-for="TipoCochera" class="control-label"></label>
                <select asp-for="TipoCochera" class="form-control" asp-items="Html.GetEnumSelectList<TipoCochera>()">
                    <option selected="selected" value="">Please select</option>
                </select>
                <span asp-validation-for="TipoCochera" class="text-danger"></span>
            </div>
            <div class="form-group">
                <label asp-for="VehiculoId" class="control-label"></label>
                @Html.DropDownList("patente", new SelectList(ViewBag.Patentes))
                <span asp-validation-for="VehiculoId" class="text-danger"></span>
            </div>
            <div class="form-group">
                <input type="submit" value="Create" class="btn btn-primary" />
            </div>
        </form>
    </div>
</div>

<div>
    <a asp-action="Index">Back to List</a>
</div>

@section Scripts {
    @{await Html.RenderPartialAsync("_ValidationScriptsPartial");}
}
